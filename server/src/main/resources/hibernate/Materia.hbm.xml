<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.ar.tempus.model">

    <class name="Materia" table="materia">

        <id name="materiaId" column="materiaId">
            <generator class="identity"/>
        </id>


        <list name="comisiones" cascade="all" fetch="join" lazy="false">
            <key column="materia_id"/>
            <list-index column="idx"/>
            <one-to-many class="Comision"/>
        </list>

        <property name="materiaNombre" column="materiaNombre" not-null="true"/>


    </class>

</hibernate-mapping>

